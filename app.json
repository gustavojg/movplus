{
  "expo": {
    "name": "movplus",
    "slug": "movplus",
    "scheme": "movplus",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.with-new-arch"
    },
    "android": {
      "package": "com.example.withnewarch"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Calistoga-Regular.ttf"]
        }
      ]
    ]
  },
  "web": {
    "bundler": "metro"
  }
}
